# Aictive Platform v2 Configuration

# AI Services (REQUIRED)
ANTHROPIC_API_KEY=your_claude_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Database (REQUIRED)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_key_here

# Background Jobs (REQUIRED for V2)
INNGEST_SIGNING_KEY=your_inngest_signing_key
INNGEST_EVENT_KEY=your_inngest_event_key

# Search (REQUIRED for V2)
MEILISEARCH_HOST=http://localhost:7700
MEILISEARCH_KEY=your_meilisearch_master_key

# Optional Services
AZURE_VISION_KEY=optional_azure_key
REDIS_URL=redis://localhost:6379
RENTVINE_API_KEY=optional_rentvine_key
SLACK_WEBHOOK_URL=optional_slack_webhook

# Settings
ENVIRONMENT=development
DEBUG_MODE=true
LOG_LEVEL=INFO
USE_MOCK_SERVICES=false

# Service Info
SERVICE_NAME=aictive-platform
SERVICE_VERSION=2.0.0
